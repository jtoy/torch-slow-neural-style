Inference:
  command: bash inference.sh
  parameters:
    -content_image:
      flag: -content_image
      type: file
      required: true
    -style_image:
      flag: -style_image
      type: file
      required: true
    -image_size:
      flag: -image_size
      type: int
      default: 512
    -tv_strength:
      flag: -tv_strength
      type: float
      default: 1e-6
    -content_weights:
      flag: -content_weights
      type: float
      default: 1.0
    -style_weights:
      flag: -style_weights
      type: float
      default: 5.0
